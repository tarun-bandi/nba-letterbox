{
  "expo": {
    "name": "Know Ball",
    "slug": "know-ball",
    "scheme": "know-ball",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#0a0a0a",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tarunbandi.knowball",
      "associatedDomains": [
        "applinks:know-ball.vercel.app",
        "applinks:know-ball.vercel.app"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0a0a"
      },
      "package": "com.tarunbandi.knowball",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "know-ball.vercel.app",
              "pathPrefix": "/game"
            },
            {
              "scheme": "https",
              "host": "know-ball.vercel.app",
              "pathPrefix": "/user"
            },
            {
              "scheme": "https",
              "host": "know-ball.vercel.app",
              "pathPrefix": "/list"
            },
            {
              "scheme": "https",
              "host": "know-ball.vercel.app",
              "pathPrefix": "/tag"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#c9a84c"
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow Know Ball to access your contacts to find friends on the app."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d4cd43fb-f1de-4556-b002-3a3429ebccb4"
      }
    }
  }
}
